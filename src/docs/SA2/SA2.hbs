<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <style>
      table { width: 800px; border-collapse: collapse; font-family: "Arial";
      table-layout: fixed; } th, td { border: 1px solid black; /* padding: 8px;
      */ text-align: left; height: 20px; } .no-border { border: none; } .header
      { text-align: center; font-weight: bold; } .map { height: 500px; } .gray {
      background-color: #c0c0c0; } .right { text-align: left; } .left {
      text-align: left; } .separator { border: none; height: 20px; } .checkbox
      span { height: 15px; width: 15px !important; border: 1px solid black;
      padding: 0 7.5px; margin: 0 20px; } .vide { border: solid 1px gainsboro;
      text-align: right; border-right: black; } .vide2 { border: solid 1px
      gainsboro; text-align: right; border-right: 1px solid black;
      border-bottom: 1px solid black; } .vide3 { border: solid 1px gainsboro;
      text-align: left; border-right: 1px solid black; border-bottom: 1px solid
      black; } .bleu { color: blue; } .multiline { /* width: 600px; */
      word-wrap: break-word; white-space: normal; vertical-align: top;
      text-align: left; } .pagebreak { page-break-after: always; }
    </style>
  </head>
  <body>
    <table>
      <tr>
        <td colspan="4" class="no-border">
          <img src="logo.png" alt="Mobilis Logo" height="40" />
        </td>
        <td colspan="4" class="header gray" style="font-size: 28px">SA-2</td>
      </tr>
      <tr>
        <td colspan="4" class="no-border"></td>
        <td
          colspan="4"
          class="header no-border"
          rowspan="3"
          style="height: 60px"
        >
          Rapport d'Identification de Candidat
        </td>
      </tr>
      <tr></tr>
      <tr></tr>
      <tr>
        <td colspan="2" class="header right gray">Nom du site:</td>
        <td colspan="2">{{nom_du_site}}</td>
        <td colspan="2" class="header right gray">Code Site:</td>
        <td colspan="2">{{code_site}}</td>
      </tr>
      <tr>
        <td colspan="2" class="header right gray">Qualité radio (1,2,3):</td>
        <td colspan="2">{{qualite_radio}}</td>
        <td colspan="2" class="header right gray">Candidat(a,b,c...):</td>
        <td colspan="2">{{candidat}}</td>
      </tr>
      <tr>
        <td colspan="2" class="separator"></td>
        <td colspan="2" class="separator"></td>
        <td colspan="2" class="header right gray">Phase:</td>
        <td colspan="2">{{phase}}</td>
      </tr>
      <tr class="separator"></tr>
      <tr>
        <td colspan="2" class="header right gray">Nom de Négociateur:</td>
        <td colspan="2">{{nom_negociateur}}</td>
        <td colspan="2" class="header right gray">Mobile:</td>
        <td colspan="2">{{mobile_negociateur}}</td>
      </tr>
      <tr>
        <td colspan="2" class="header right gray">Société:</td>
        <td colspan="2" colspan="2">{{societe_negociateur}}</td>
        <td colspan="2" class="header right gray">Téléphone:</td>
        <td colspan="2">{{telephone_negociateur}}</td>
      </tr>
      <tr class="separator"></tr>
      <tr>
        <td colspan="8" class="header gray">Informations Générales</td>
      </tr>
      <tr>
        <td colspan="2" rowspan="2" class="header left gray">
          Adresse du Site:
        </td>
        <td colspan="2" rowspan="2">{{adresse_site}}</td>
        <td colspan="2" class="header left gray">Commune:</td>
        <td colspan="2">{{commune}}</td>
      </tr>
      <tr>
        <td colspan="2" class="header right gray">Wilaya:</td>
        <td colspan="2">{{wilaya}}</td>
      </tr>
      <tr class="separator"></tr>
      <tr>
        <td colspan="2" class="header left gray">Nom du propriétaire:</td>
        <td colspan="2">{{nom_proprietaire}}</td>
        <td colspan="2" class="header right gray">Téléphone</td>
        <td colspan="2">{{telephone_proprietaire}}</td>
      </tr>

      <tr>
        <td colspan="2" rowspan="2" class="header left gray">
          Adresse du propriétaire: :
        </td>
        <td colspan="2" rowspan="2">{{adresse_proprietaire}}</td>
        <td colspan="2" class="header right gray">Mobile:</td>
        <td colspan="2">{{mobile_proprietaire}}</td>
      </tr>
      <tr>
        <td colspan="2" class="header right gray">Fax:</td>
        <td colspan="2">{{fax_proprietaire}}</td>
      </tr>
      <tr class="separator"></tr>
      <tr>
        <td colspan="2" class="header left gray">Personne a contacter:</td>
        <td colspan="2">{{personne_a_contacter}}</td>
        <td colspan="2" class="header right gray">Téléphone</td>
        <td colspan="2">{{telephone_contact}}</td>
      </tr>

      <tr>
        <td colspan="2" rowspan="2" class="header left gray">
          Adresse du contact:
        </td>
        <td colspan="2" rowspan="2">{{adresse_contact}}</td>
        <td colspan="2" class="header right gray">Mobile:</td>
        <td colspan="2">{{mobile_contact}}</td>
      </tr>
      <tr>
        <td colspan="2" class="header right gray">Fax:</td>
        <td colspan="2">{{fax_contact}}</td>
      </tr>
      <tr class="separator"></tr>
      <tr>
        <td colspan="2" rowspan="3" class="header left gray">
          Autres Informations :
        </td>
        <td
          class="multiline"
          colspan="6"
          rowspan="3"
          style="border-bottom: none; height: 60px"
        >
          {{autres_informations}}
        </td>
      </tr>
      <tr></tr>
      <tr></tr>
      <tr>
        <td colspan="8" class="header gray">Détails du Candidat</td>
      </tr>
      <tr>
        <td colspan="2" class="header left gray bleu">Type de site:</td>
        <td colspan="2">{{type_de_site}}</td>
        <td colspan="2" class="header right gray">Hauteur:</td>
        <td colspan="2">{{hauteur}}</td>
      </tr>
      <tr class="separator"></tr>
      <tr>
        <td class="vide3 header" colspan="4">cas d'un batiment</td>
        <td colspan="2" class="header left gray">Nombre d'etage:</td>
        <td colspan="2">{{nombre_etage}}</td>
      </tr>
      <tr>
        <td colspan="2" class="header left gray">Année de construction:</td>
        <td colspan="2">{{annee_construction}}</td>
        <td colspan="2" class="header left gray bleu">Toit plat:</td>
        <td colspan="2">{{toit_plat}}</td>
      </tr>
      <tr>
        <td colspan="2" class="header left gray bleu">Local disponible:</td>
        <td colspan="2">{{local_disponible}}</td>
        <td colspan="2" class="header right gray">Dimensions Local:</td>
        <td colspan="2">{{dimensions_local}}</td>
      </tr>
      <tr class="separator"></tr>
      <tr>
        <td colspan="4" class="header left gray">
          Coordonnées du site théorique :
        </td>

        <td colspan="4" class="header right gray">Coordonnées du candidat</td>
      </tr>
      <tr>
        <td colspan="2" class="header left gray">Longitude::</td>
        <td colspan="2">{{longitude_site_theorique}}</td>
        <td colspan="2" class="header right gray">Longitude:</td>
        <td colspan="2">{{longitude_candidat}}</td>
      </tr>
      <tr>
        <td colspan="2" class="header left gray">Latitude:</td>
        <td colspan="2">{{latitude_site_theorique}}</td>
        <td colspan="2" class="header right gray">Latitude:</td>
        <td colspan="2">{{latitude_candidat}}</td>
      </tr>
      <tr>
        <td colspan="2" class="header left gray bleu">
          Source des coordonnées:
        </td>
        <td colspan="2">{{source_coordonnees}}</td>
        <td colspan="2" class="header right gray">Altitude:</td>
        <td colspan="2">{{altitude}}</td>
      </tr>
      <tr class="separator"></tr>
      <tr>
        <td colspan="2" class="header left gray bleu">Zone:</td>
        <td colspan="2">{{zone}}</td>
        <td colspan="2" class="header left gray bleu">Environnement:</td>
        <td colspan="2">{{environnement}}</td>
      </tr>
      <tr>
        <td colspan="2" rowspan="2" class="header left gray">
          Antennes presents sur le site:
        </td>
        <td colspan="6" rowspan="2" class="multiline" style="height: 40px">
          {{antennes_sur_site}}
        </td>
      </tr>
      <tr class="separator"></tr>
      <tr>
        <td colspan="2" rowspan="2" class="header left gray">
          Autre sites environnants:
        </td>
        <td colspan="6" rowspan="2" class="multiline" style="height: 40px">
          {{autres_sites_environnants}}
        </td>
      </tr>
      <tr class="separator">
        <!-- <td colspan="6"></td> -->
      </tr>
      <tr class="separator"></tr>
      <tr>
        <td colspan="2" class="header left gray bleu">Type de propriété:</td>
        <td colspan="2">{{type_propriete}}</td>
        <td colspan="2" class="header left gray bleu">Acces 24/24:</td>
        <td colspan="2">{{acces_24_24}}</td>
      </tr>
      <tr>
        <td colspan="2" class="header left gray">Date estimée de signature:</td>
        <td colspan="2">{{date_estimee_signature}}</td>
        <td colspan="2" class="header left gray bleu">
          Fiabilité de negociatin:
        </td>
        <td colspan="2">{{fiabilite_negociation}}</td>
      </tr>

      <tr class="separator"></tr>
      <tr>
        <td colspan="8" class="header gray">Document(cocher si presents):</td>
      </tr>

      <tr>
        <td colspan="2" rowspan="4" class="vide3">
          Diagramme des obstacles<br />
          Croquis sommaire
          <br />
          Carte
          <br />
          Photo d'ensemble
        </td>
        <td colspan="2">{{#if diag_obstacles}} x {{/if}}</td>
        <td colspan="2" rowspan="4" class="vide3">
          Panoramique 360°
          <br />
          Photo de local
          <br />
          Photo de la terrasse
          <br />
          Autres
        </td>
        <td colspan="2">{{#if panoramique}} x {{/if}}</td>
      </tr>
      <tr>
        <td colspan="2">{{#if croquis_sommaire}} x {{/if}}</td>
        <td colspan="2">{{#if photo_local}} x {{/if}}</td>
      </tr>
      <tr>
        <td colspan="2">{{#if carte}} x {{/if}}</td>
        <td colspan="2">{{#if photo_terasse}} x {{/if}}</td>
      </tr>
      <tr class="pagebreak">
        <td colspan="2">{{#if photo_ensemble}} x {{/if}}</td>
        <td colspan="2">{{#if autres}} x {{/if}}</td>
      </tr>

      <tr>
        <td colspan="2" class="header gray right">validation Acquisition:</td>
        <td colspan="6" class="checkbox">
          Validé
          <span>{{#if acq_validated}} x {{/if}}</span>
          Rejeté
          <span>{{#if acq_rejected}} x {{/if}}</span>
          Gelé
          <span>{{#if acq_gele}} x {{/if}}</span>
        </td>
      </tr>

      <!-- </tr> -->

      <tr>
        <td colspan="2" class="header gray left">Responsable Acq:</td>
        <td colspan="2">{{responsable_acq}}</td>
        <td colspan="4" rowspan="2">Signature :</td>
      </tr>
      <tr>
        <td colspan="2" class="header gray left">Date:</td>
        <td colspan="2">{{data_validation_acq}}</td>
      </tr>
      <tr>
        <td colspan="2" class="header gray right">validation Radio:</td>
        <td colspan="6" class="checkbox">
          Validé
          <span>{{#if rad_validated}} x {{/if}}</span>
          Rejeté
          <span>{{#if rad_rejected}} x {{/if}}</span>
          Gelé
          <span>{{#if rad_gele}} x {{/if}}</span>
        </td>
      </tr>

      <!-- </tr> -->

      <tr>
        <td colspan="2" class="header gray left">Responsable RF:</td>
        <td colspan="2">{{responsable_RF}}</td>
        <td colspan="4" rowspan="2">Signature :</td>
      </tr>
      <tr>
        <td colspan="2" class="header gray left">Date:</td>
        <td colspan="2">{{date_validation_RF}}</td>
      </tr>

      <!-- </tr> -->
      <tr>
        <td colspan="2" class="header gray left">Négociateur sous-traitant</td>
        <td colspan="2">{{negociateur_soustraitant}}</td>
        <td colspan="4" rowspan="2">Signature :</td>
      </tr>
      <tr>
        <td colspan="2" class="header gray left">Date:</td>
        <td colspan="2">{{date_negociateur_soustraitant}}</td>
      </tr>
    </table>
  </body>
</html>